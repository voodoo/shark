#myCarousel.carousel.slide{"data-ride" => "carousel"}
  / Indicators
  %ol.carousel-indicators
    %li.active{"data-slide-to" => "0", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "1", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "2", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "3", "data-target" => "#myCarousel"}
  .carousel-inner
    .item.active
      %img{:alt => "Fender Jaguar", :src => "/img/dbz/hf.jpg"}/
      .container
        .carousel-caption
          %h2 Dbz Hailfire
          %p 25" scale
          %p Duncan Pickups
    .item
      %img{:alt => "Second slide", :src => "/img/dbz/hf-walnut.png"}/
      .container
        .carousel-caption
          %h2 Charcoal
          %p
          / %p
          /   %a.btn.btn-lg.btn-primary{:href => "#", :role => "button"} Learn more
    .item
      %img{:alt => "Digitally remove body", :src => "/img/dbz/mod-hf.png"}/
      .container
        .carousel-caption
          %h2 Digital prototype
    .item
      %img{:alt => "Third slide", :src => "/img/dbz/mod-hf.png"}/
      .container
        .carousel-caption
          %h2 Remove some bout and butt          

  %a.left.carousel-control{"data-slide" => "prev", :href => "#myCarousel", :role => "button"}
    %span.glyphicon.glyphicon-chevron-left
  %a.right.carousel-control{"data-slide" => "next", :href => "#myCarousel", :role => "button"}
    %span.glyphicon.glyphicon-chevron-right
/ /.carousel